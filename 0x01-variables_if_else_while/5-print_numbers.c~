#include <stdio.h>
/*                                                                                                                                           
 * main - prints base10 num from zero                                                                                                        
 *                                                                                                                                           
 * Return: 0 (success)                                                                                                                       
 */
int main(void)
{
	int x;

	for (x = 0; x <= 10; x++)
	{
		printf("%d", x);

	}
	return (0);
}
